# Example AI Secret Configuration
# To create the actual secret, run:
# kubectl create secret generic jhb-streampulse-ai-secret \
#   --from-literal=ANTHROPIC_API_KEY='your-api-key-here' \
#   -n jhb-streampulse

apiVersion: v1
kind: Secret
metadata:
  name: jhb-streampulse-ai-secret
  namespace: jhb-streampulse
type: Opaque
stringData:
  ANTHROPIC_API_KEY: "your-anthropic-api-key-here"
